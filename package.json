{
  "name": "@xeel-dev/cli",
  "version": "0.0.13",
  "description": "Xeel Command Line Interface",
  "license": "MIT",
  "bugs": "https://github.com/xeel-dev/xeel-cli/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xeel-dev/xeel-cli.git"
  },
  "maintainers": [
    {
      "email": "felix@xeel.dev",
      "name": "Felix"
    }
  ],
  "bin": {
    "xeel": "bin/run.js"
  },
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./ecosystem-support": "./dist/ecosystems/index.js"
  },
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "oclif manifest",
    "start": "./bin/dev.js",
    "test": "vitest run"
  },
  "engines": {
    "node": "20"
  },
  "packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@types/node": "20",
    "@vitest/coverage-v8": "^2.1.2",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "shx": "^0.3.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@oclif/core": "^4.0.27",
    "@oclif/plugin-help": "^6.2.14",
    "@oclif/plugin-plugins": "^5.4.14",
    "chalk": "^5.3.0",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "open": "^10.1.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "xeel",
    "dirname": "xeel-cli",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "scope": "@xeel-dev",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  }
}
